while wait(0.05) do
    local args = {
        [1] = {
            ["id"] = "cut",
            ["identifier"] = "basic",
            ["quantity"] = 0.00001,
            ["field"] = "blue"
        }
    } 
    game:GetService("ReplicatedStorage").Remotes.GenericFunction:InvokeServer(unpack(args))
end
